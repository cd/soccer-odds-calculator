{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "google",
        "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 8
    },
    "rules": {
      "prettier/prettier": "error",
      "require-atomic-updates": "off",
      "max-len": ["error", {"code": 80, "ignoreUrls": true}]
    }
}